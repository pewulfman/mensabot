// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model members {
  id          Int @default(autoincrement()) @id
  name        String
  email       String
  region      String
  mensaTag    Int @unique
  discordTag  String @unique
  membership  Boolean
  inter       Boolean
}

model pendingMembers {
  id          Int @default(autoincrement()) @id
  name        String
  email       String
  region      String
  mensaTag    Int @unique
  discordTag  String @unique
  membership  Boolean
  code        String
  trials      Int
  inter       Boolean
}
 
model guild {
  id         Int @default(autoincrement()) @id
  name       String
  discordTag String @unique
  roleTag    String
}