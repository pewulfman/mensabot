// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model members {
  id          Int @default(autoincrement()) @id
  name        String
  email       String
  region      String
  mensaId     Int @unique
  membership  Boolean
  inter       Boolean
  discordId   String?
  code        String?
}

model guild {
  id         Int @default(autoincrement()) @id
  name       String
  discordId  String @unique
  roleId     String
}